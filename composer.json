{
  "name": "pomm-project/pomm-bundle",
  "type": "symfony-bundle",
  "description": "The Symfony6 bundle for Pomm3",
  "keywords": ["orm", "postgresql", "database", "symfony", "pomm"],
  "homepage": "http://www.pomm-project.org",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas JOSEPH",
      "email": "nicolas.joseph@homecomputing.fr"
    },
    {
      "name": "GrÃ©goire HUBERT",
      "email": "hubert.greg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "jdorn/sql-formatter": "~1.2",
    "pomm-project/model-manager": "~3.0",
    "pomm-project/cli": "~3.0",
    "pomm-project/pomm-symfony-bridge": "~4.0",
    "symfony/framework-bundle": "^6.0"
  },
  "require-dev": {
    "symfony/console": "^6.0",
    "squizlabs/php_codesniffer": "~2.7"
  },
  "suggest": {
      "symfony/web-profiler-bundle": "Display queries log",
      "sensio/distribution-bundle": "To use the entity param converter"
  },
  "autoload": {
    "psr-4": {
      "PommProject\\PommBundle\\": "sources/lib/"
    }
  },
  "extra": {
      "branch-alias": {
          "dev-master": "4.1-dev"
      }
  },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DoudDNC/ModelManager"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DoudDNC/Cli"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DoudDNC/pomm-symfony-bridge"
        }
    ]
}

